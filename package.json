{
  "name": "whatsapp-bot",
  "version": "3.0.0",
  "description": "Whatsapp Bot - Node Js",
  "main": "./dist/main.js",
  "private": true,
  "scripts": {
    "build": "rimraf dist && babel src --copy-files --extensions .ts --out-dir dist && rimraf dist/types",
    "dev": "rimraf dist && tsc-watch -p ./tsconfig.dev.json --outDir ./dist --onSuccess \"node .\" --compiler typescript/bin/tsc --noClear",
    "format": "prettier --write \"src/**/*.+(ts|json)\"",
    "generateLang": "node -r ./generateLang ./generateLang.arugaz",
    "pm2": "pm2 start ecosystem.config.js && pm2 logs whatsappbot --raw",
    "start": "node ./dist/main.js"
  },
  "author": "arugaz",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArugaZ/whatsapp-bot.git"
  },
  "bugs": {
    "url": "https://github.com/ArugaZ/whatsapp-bot/issues"
  },
  "homepage": "https://github.com/ArugaZ/whatsapp-bot",
  "license": "Apache-2.0",
  "babel": {
    "comments": false,
    "generatorOpts": {
      "compact": true
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "bugfixes": true,
          "modules": "commonjs"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@adiwajshing/baileys": "git+https://gitlab.com/arugaz/Baileys",
    "@arugaz/translator": "1.0.0-z.4",
    "@prisma/client": "4.6.1",
    "cfonts": "3.1.1",
    "cheerio": "1.0.0-rc.12",
    "croner": "5.3.5",
    "form-data": "4.0.0",
    "qrcode-terminal": "0.12.0",
    "sharp": "0.30.7",
    "undici": "5.13.0"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/node": "18.11.9",
    "prettier": "2.8.0",
    "prisma": "4.6.1",
    "rimraf": "3.0.2",
    "tsc-watch": "5.0.3",
    "typescript": "4.9.3"
  }
}
