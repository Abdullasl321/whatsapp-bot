{
  "name": "whatsapp-bot",
  "version": "4.0.0",
  "description": "Whatsapp Bot - Node Js",
  "main": "./dist/main.js",
  "private": true,
  "scripts": {
    "build": "rimraf dist && babel src --copy-files --extensions .ts --out-dir dist && rimraf dist/types && node uglify.script.js",
    "dev": "tsc-watch -p ./tsconfig.json --outDir ./dist --onSuccess \"node .\" --compiler typescript/bin/tsc",
    "format": "prettier --write \"src/**/*.+(ts|json)\"",
    "postinstall": "rimraf node_modules/@adiwajshing/baileys/node_modules/libsignal && move-cli node_modules/libsignal node_modules/@adiwajshing/baileys/node_modules/libsignal",
    "start": "node ."
  },
  "author": "arugaz",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArugaZ/whatsapp-bot.git"
  },
  "bugs": {
    "url": "https://github.com/ArugaZ/whatsapp-bot/issues"
  },
  "homepage": "https://github.com/ArugaZ/whatsapp-bot",
  "license": "Apache-2.0",
  "babel": {
    "comments": false,
    "generatorOpts": {
      "minified": true
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "bugfixes": true
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@adiwajshing/baileys": "git+https://github.com/adiwajshing/baileys",
    "@arugaz/translator": "1.0.0-z.2",
    "@prisma/client": "4.6.1",
    "axios": "1.1.3",
    "cfonts": "3.1.1",
    "cheerio": "1.0.0-rc.12",
    "form-data": "4.0.0",
    "qrcode-terminal": "0.12.0"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/node": "18.11.9",
    "libsignal": "git+https://github.com/arugaz/libsignal-node",
    "move-cli": "2.0.0",
    "prettier": "2.7.1",
    "prisma": "4.6.1",
    "rimraf": "3.0.2",
    "tsc-watch": "^5.0.3",
    "typescript": "4.9.3",
    "uglify-js": "3.17.4"
  }
}
