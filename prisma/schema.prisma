// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

// For production you should use other database like postgresql or mongodb or something similar
datasource db {
  provider = "sqlite"
  url      = "file:../database/database.db?socket_timeout=10&connection_limit=1"
}

model Session {
  sessionId String  @unique
  session   String?
}
